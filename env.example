# iOS Configuration
APPLE_ID=your_apple_id@example.com
APPLE_ID_PASSWORD=your_app_specific_password
IOS_TEAM_ID=your_team_id
MATCH_PASSWORD=your_match_password

# Android Configuration
ANDROID_KEYSTORE_PATH=path/to/your/keystore.jks
ANDROID_KEYSTORE_PASSWORD=your_keystore_password
ANDROID_KEY_ALIAS=your_key_alias
ANDROID_KEY_PASSWORD=your_key_password

# Google Play Console
GOOGLE_PLAY_SERVICE_ACCOUNT_JSON={"type":"service_account","project_id":"your_project_id",...}

# Fastlane Session (optional, for 2FA)
FASTLANE_SESSION=your_fastlane_session_token

